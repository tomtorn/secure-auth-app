{
  "name": "secure-auth-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --max-warnings=0",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "NODE_OPTIONS='--experimental-vm-modules' npx jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' npx jest --watch",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "docker:build": "docker build -t secure-auth-server .",
    "docker:run": "docker run --rm -p 4000:4000 --env-file .env secure-auth-server",
    "docker:run:detached": "docker run -d -p 4000:4000 --env-file .env --name secure-auth-server secure-auth-server",
    "cleanup-orphans": "tsx src/scripts/cleanup-orphans.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.948.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.948.0",
    "@aws-sdk/client-ecs": "^3.948.0",
    "@prisma/client": "^5.22.0",
    "@secure-auth/schemas": "*",
    "@sentry/node": "^10.30.0",
    "@supabase/supabase-js": "^2.47.10",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.0",
    "helmet": "^8.0.0",
    "inherits": "^2.0.4",
    "ioredis": "^5.8.2",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/pino-http": "^5.8.4",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.2.5",
    "prisma": "^5.22.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  }
}
